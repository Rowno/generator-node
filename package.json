{
  "name": "@rowno/generator-node",
  "version": "9.0.1",
  "description": "Generic Yeoman generator for creating Node.js servers and modules.",
  "author": "Roland Warmerdam (https://roland.codes)",
  "keywords": [
    "yeoman-generator",
    "node"
  ],
  "repository": "Rowno/generator-node",
  "license": "MIT",
  "scripts": {
    "test": "xo && ava"
  },
  "files": [
    "app"
  ],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "gh-user": "^3.0.0",
    "globby": "^8.0.1",
    "moment": "^2.22.2",
    "yeoman-generator": "^2.0.5"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-eslint": "^8.2.5",
    "eslint-config-xo-react": "^0.16.0",
    "eslint-plugin-react": "^7.10.0",
    "execa": "^0.10.0",
    "fs-extra": "^6.0.1",
    "xo": "^0.21.1",
    "yeoman-test": "^1.8.0"
  },
  "xo": {
    "parser": "babel-eslint",
    "extends": "xo-react",
    "prettier": true,
    "space": true,
    "semicolon": false,
    "overrides": [
      {
        "files": "app/templates/**/*.js",
        "rules": {
          "import/no-extraneous-dependencies": "off",
          "import/no-unresolved": "off",
          "ava/no-ignored-test-files": "off"
        }
      }
    ]
  },
  "ava": {
    "files": [
      "test/*.js"
    ]
  }
}
