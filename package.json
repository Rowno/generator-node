{
  "name": "@rowno/generator-node",
  "version": "10.1.0",
  "description": "Generic Yeoman generator for creating Node.js servers and modules.",
  "author": "Roland Warmerdam (https://roland.codes)",
  "keywords": [
    "yeoman-generator",
    "node"
  ],
  "repository": "Rowno/generator-node",
  "license": "MIT",
  "scripts": {
    "test": "xo && ava"
  },
  "files": [
    "app"
  ],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "gh-user": "^4.0.0",
    "globby": "^10.0.1",
    "moment": "^2.24.0",
    "yeoman-generator": "^4.0.2"
  },
  "devDependencies": {
    "ava": "2.4.0",
    "babel-eslint": "^10.0.3",
    "eslint-config-xo-react": "^0.20.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "execa": "^2.0.4",
    "fs-extra": "^8.1.0",
    "xo": "^0.24.0",
    "yeoman-test": "^2.0.0"
  },
  "xo": {
    "parser": "babel-eslint",
    "extends": "xo-react",
    "prettier": true,
    "space": true,
    "semicolon": false,
    "settings": {
      "react": {
        "version": "latest"
      }
    },
    "overrides": [
      {
        "files": "app/templates/**/*.js",
        "rules": {
          "import/no-extraneous-dependencies": "off",
          "import/no-unresolved": "off",
          "ava/no-ignored-test-files": "off"
        }
      }
    ]
  },
  "ava": {
    "files": [
      "test/*.js"
    ]
  }
}
