{
  "name": "@rowno/generator-node",
  "version": "10.1.0",
  "description": "Generic Yeoman generator for creating Node.js servers and modules.",
  "author": "Roland Warmerdam (https://roland.codes)",
  "keywords": [
    "yeoman-generator",
    "node"
  ],
  "repository": "Rowno/generator-node",
  "license": "MIT",
  "scripts": {
    "test": "yarn lint && jest",
    "lint": "eslint '**/*.js'"
  },
  "files": [
    "app"
  ],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "gh-user": "^4.0.0",
    "globby": "^10.0.1",
    "moment": "^2.24.0",
    "yeoman-generator": "^4.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-xo": "^0.27.1",
    "eslint-config-xo-react": "^0.20.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.1.0",
    "execa": "^2.0.4",
    "fs-extra": "^8.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "yeoman-test": "^2.0.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "xo/esnext",
      "xo-react",
      "prettier",
      "prettier/react"
    ],
    "env": {
      "node": true,
      "jest": true
    },
    "settings": {
      "react": {
        "version": "latest"
      }
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "jest": {
    "testEnvironment": "node",
    "clearMocks": true
  }
}
